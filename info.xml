<?xml version="1.0"?>
<extension key="net.bund.civicrm.events" type="module">
  <file>events</file>
  <name>BUND Events</name>
  <description>Provides features and customisations for BUND events</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/net.bund.civicrm.events</url>
    <url desc="Documentation">https://github.com/systopia/net.bund.civicrm.events</url>
    <url desc="Support">https://github.com/systopia/net.bund.civicrm.events/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-08-09</releaseDate>
  <version>1.3.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.65</ver>
  </compatibility>
  <comments/>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Events</namespace>
    <format>23.02.1</format>
  </civix>
  <requires>
    <ext>de.systopia.remoteevent</ext>
  </requires>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>scan-classes@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Events_Upgrader</upgrader>
</extension>
